  \n   \n \n    return(\n        <section className=\"WhoWeHelp\">\n            <WhoWeHelp_Title />\n            <p className=\"WhoWeHelp_A\"></p>\n            <WhoWeHelp_Fundactions />            \n        </section>\n    )\n}\n\nexport default WhoWeHelp","/home/adam/Pulpit/kodowanie/CodeMaster/code-master/src/components/Main/Main.js",[],"/home/adam/Pulpit/kodowanie/CodeMaster/code-master/src/components/Log/Log.js",["166","167"],"/home/adam/Pulpit/kodowanie/CodeMaster/code-master/src/components/Contact/Contact_Form.js",["168"],"import React, {useEffect, useState} from \"react\"\n\nlet Contact_Form = ({ addP }) => {\n    let [name, setName] = useState({name: \"\"});\n    let [email, setEmail] = useState({email: \"\"});\n    let [message, setMessage] = useState({message: \"\"});\n    let [succesSend, setSuccessSend] = useState(false)\n    let [errorSend, setErrorSend] = useState(false)\n    let [borderBottomColor, setBorderBottomColor] = useState(\"\")\n   \n    const handleChangeName = (e) => {\n        setName(e.target.value);\n    };\n    const handleChangeEmail = (e) => {\n        setEmail(e.target.value);\n    };\n    const handleChangeMessage = (e) => {\n        setMessage(e.target.value);\n    };\n\n    \n\n    const handleSubmit = (e) => {\n\n        e.preventDefault();\n        addP({name,email,message})\n        // console.log({name,email,message})\n        fetch(\"https://fer-api.coderslab.pl/v1/portfolio/contact\",{\n            method:'POST',\n            headers:{\n                \"Content-Type\": \"application/json\"\n            },\n            body: JSON.stringify({name,email,message})\n        })\n        .then(data=>data.json())\n        .then(data=>{\n            if(data.status === \"success\"){\n                console.log(\"suces\")\n                setSuccessSend(true)\n                setErrorSend(false)\n                setBorderBottomColor(\"#737373\")\n\n            }else{\n                setErrorSend(true)\n                setSuccessSend(false)\n                setBorderBottomColor(\"#DC143C\")\n            }\n        })\n        \n        \n       \n    };\n\n    function SendClear(){\n        setTimeout(() => {\n            setSuccessSend(false)\n            setErrorSend(false)\n            setBorderBottomColor(\"#000000\")\n        },8000)\n    }\n        \n    \n    return(\n        <div className=\"Contact_Form\">\n            <h2>Skontaktuj sie z nami</h2>\n            {succesSend && <h3 classSend=\"succesSend\" style={{color: \"green\", marginRight: 30}}>wysłano pomyślnie</h3>}\n            \n            <p className=\"Contact_Form_A\"></p>\n            <div className=\"Form\">\n                <form onSubmit={handleSubmit} >\n                <div className=\"Form-A\">\n                   \n                    <label > Wpisz swoje imie <br/>\n                \n                    <input style={{borderBottomColor: borderBottomColor}} type=\"text\" id=\"name\" onChange={handleChangeName}></input>\n                    {errorSend && <h3 className=\"errorSend\"  style={{color: \"red\"}}>Podane imię jest nieprawidłowe</h3>}\n                    </label><br/>\n                        \n                    <label >Wpisz swoj email<br/>\n                    <input style={{borderBottomColor: borderBottomColor}} type=\"text\" id=\"email\" onChange={handleChangeEmail}></input>\n                    {errorSend && <h3 className=\"errorSend\" style={{color: \"red\"}}>Podane e-mail jest nieprawidłowe</h3>}\n\n                    </label><br/>\n                </div>\n                <div className=\"Form-B\">\n                    <label >Wpisz swoją wiadomość<br/>\n                    <textarea style={{borderBottomColor: borderBottomColor}} type=\"text\" className=\"input\" onChange={handleChangeMessage}></textarea>\n                    {errorSend && <h3 className=\"errorSend\"  style={{color: \"red\"}}>Wiadomość musi mieć conajmniej 120 znaków</h3>}\n\n                    </label><br/>\n                    <input className=\"input_Send\" type=\"submit\" value=\"wyślij\" onClick={SendClear}/>\n                </div>\n                \n                </form>\n            </div>\n        </div>\n    )\n}\nexport default Contact_Form","/home/adam/Pulpit/kodowanie/CodeMaster/code-master/src/components/SimpleSteps/SimpleStep_Title.js",[],"/home/adam/Pulpit/kodowanie/CodeMaster/code-master/src/components/SimpleSteps/SimpleStepButton.js",[],"/home/adam/Pulpit/kodowanie/CodeMaster/code-master/src/components/Contact/Contact_Picture.js",[],"/home/adam/Pulpit/kodowanie/CodeMaster/code-master/src/components/Contact/Contac_Footer.js",[],"/home/adam/Pulpit/kodowanie/CodeMaster/code-master/src/components/AboutUs/AboutUs_Picture.js",[],"/home/adam/Pulpit/kodowanie/CodeMaster/code-master/src/components/AboutUs/AboutUs_Text.js",["169"],"import WhoWEHelp from \"./AboutUs\"\n\nlet AboutUs_Text = () =>{\n    return(\n        <div className=\"AboutUs_Text\">\n            <h3>O Nas</h3>\n            <p className=\"AboutUs_Text_Icon-One\"></p>\n            <p>Nori grape silver beet broccoli kombu beet greens fava bean potato quandong celery. Bunya nuts black-eyed pea prairie turnip leek lentil turnip greens parsnip.\n                </p>\n            <p className=\"AboutUs_Text_Icon\"></p>\n        </div>\n    )\n}\n\nexport default AboutUs_Text","/home/adam/Pulpit/kodowanie/CodeMaster/code-master/src/components/Home/HomeCenter.js",["170","171","172","173","174"],"import HomeHeader from \"./homeHeader\"\nimport {\n    HashRouter,\n    Route,\n    Link,\n    Switch,\n    NavLink,\n} from 'react-router-dom';\n\n\nlet HomeCenter = () => {\n    return(\n        <div className=\"xx\">\n        <HomeHeader />\n        <section className=\"HomeCenter\">\n            <div className=\"HomeCenter-One\">\n                <p className=\"HomeCenter_Text\">Zacznij pomagać!</p>\n                <p className=\"HomeCenter_Text\">Oddaj niechciane rzeczy w zaufane ręce</p>\n                <p className=\"HomeC